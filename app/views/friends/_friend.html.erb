<div id="<%= dom_id friend %>">
 <% if  friend.user == current_user%>
  <p>
    <strong>First name:</strong>
    <%= friend.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= friend.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= friend.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= friend.phone %>
  </p>

  <p>
    <strong>Twitter:</strong>
    <%= friend.twitter %>
  </p>
  <p>
    <strong>User_id:</strong>
    <%= friend.user_id %>
  </p>

<%end%>
</div>
